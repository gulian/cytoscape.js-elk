{
  "name": "cytoscape-elk",
  "version": "1.1.10",
  "description": "The ELK layout algorithm adapter for Cytoscape.js",
  "main": "dist/cytoscape-elk.js",
  "author": {
    "name": "John Stephenson"
  },
  "scripts": {
    "gh-pages:demo": "cpy demo.html . --rename=index.html",
    "gh-pages:deploy": "gh-pages -d . -v node_modules/**",
    "gh-pages:clean": "rimraf index.html",
    "copyright": "update license",
    "lint": "eslint src",
    "build": "webpack --mode=production",
    "build:min": "cross-env MIN=true webpack --mode=production",
    "build:release": "run-s build copyright",
    "watch": "webpack --progress --watch --mode=development",
    "dev": "webpack-dev-server --open",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfstephe/cytoscape.js-elk.git"
  },
  "keywords": [
    "cytoscape",
    "cytoscape-extension"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfstephe/cytoscape.js-elk/issues"
  },
  "homepage": "https://github.com/jfstephe/cytoscape.js-elk",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "camelcase": "^5.3.1",
    "chai": "4.2.0",
    "cpy-cli": "^3.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "mocha": "7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "update": "^0.7.4",
    "updater-license": "^1.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "cytoscape": "^3.2.0"
  },
  "dependencies": {
    "clean": "^4.0.2",
    "elkjs": "^0.5.1"
  },
  "prettier": {
    "singleQuote": true
  }
}
